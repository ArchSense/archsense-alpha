{
  "name": "@archsense/scout",
  "version": "0.2.5-rc.0",
  "description": "Generate services graph",
  "main": "dist/index.js",
  "bin": {
    "scout": "./bin/cli.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\"",
    "release": "npm run build && npm publish",
    "rc": "npm run build && npm version prerelease --preid=rc && npm publish --tag next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArchSense/archsense-mono"
  },
  "keywords": [
    "architecture",
    "cli",
    "visualization",
    "nestjs"
  ],
  "author": "Sergey Bolshchikov<sergey@bolshchikov.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArchSense/archsense-mono/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "ts-morph": "^18.0.0",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 100,
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
